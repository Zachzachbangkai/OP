-- In a LocalScript (client side)
local TeleportService = game:GetService("TeleportService")

-- Suppose you have the share link:
local shareLink = "https://www.roblox.com/share?code=7623dddb65134247a02c116c124b9c5c&type=Server"

-- Function to extract the code from URL
local function extractCodeFromUrl(url)
    -- this is simplistic; real parsing is more robust
    for code in url:gmatch("code=([A-Za-z0-9]+)") do
        return code
    end
    return nil
end

local serverCode = extractCodeFromUrl(shareLink)
if serverCode then
    -- Now attempt to teleport the player using that server code
    -- You need the placeId of the game you want to teleport into.
    local placeId = game.PlaceId  -- or whichever place
    local teleportData = {
        serverId = serverCode
    }
    -- This will try to teleport to that specific server
    TeleportService:TeleportToPrivateServer(placeId, serverCode, {game.Players.LocalPlayer}, teleportData)
else
    warn("Could not find server code in link")
end
