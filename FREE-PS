-- DebugTeleport.lua (LocalScript in StarterGui)
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local PLACE_ID = game.PlaceId
local PRIVATE_SERVER_CODE = "7623dddb65134247a02c116c124b9c5c" -- your share link code

local success, err = pcall(function()
    TeleportService:TeleportToPrivateServer(PLACE_ID, PRIVATE_SERVER_CODE)
end)

if not success then
    warn("Teleport failed:", err)
    -- optional: show a GUI notification to the player
    local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
    local label = Instance.new("TextLabel", gui)
    label.Size = UDim2.new(0,400,0,60)
    label.Position = UDim2.new(0.5,-200,0,50)
    label.Text = "Teleport failed: "..tostring(err)
    wait(6)
    gui:Destroy()
end
