--[[
    Roblox Private Server Join Button
    Author: (Your Name)
    Description:
        Displays a GUI button that lets players
        teleport to your private server when clicked.
]]

-- // Services
local TeleportService = game:GetService("TeleportService")

-- // Configuration
local PLACE_ID = game.PlaceId
local PRIVATE_SERVER_CODE = "7623dddb65134247a02c116c124b9c5c"

-- // Create Button
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local button = Instance.new("TextButton")
button.Size = UDim2.new(0, 200, 0, 50)
button.Position = UDim2.new(0.5, -100, 0.5, -25)
button.Text = "Join Private Server"
button.Parent = screenGui

-- // Button Function
button.MouseButton1Click:Connect(function()
    TeleportService:TeleportToPrivateServer(PLACE_ID, PRIVATE_SERVER_CODE)
end)
